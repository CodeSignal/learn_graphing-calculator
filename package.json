{
  "name": "bespoke-template",
  "version": "0.0.2",
  "description": "Bespoke template with local development server and WebSocket messaging",
  "main": "server.js",
  "scripts": {
    "start": "npm run start:prod",
    "start:prod": "IS_PRODUCTION=true node server.js",
    "start:dev": "concurrently \"npm run dev:vite\" \"npm run dev:api\"",
    "dev:vite": "vite",
    "dev:api": "PORT=3001 node server.js",
    "build": "vite build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "bespoke",
    "template",
    "development",
    "server",
    "websocket"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "katex": "^0.16.11",
    "mathjs": "^12.4.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jsdom": "^27.3.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.15"
  }
}
